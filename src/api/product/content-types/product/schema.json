{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "float"
    },
    "featured": {
      "type": "boolean",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "product"
    },
    "did": {
      "type": "uid",
      "targetField": "name"
    },
    "slug": {
      "type": "string",
      "unique": true
    },
    "colors": {
      "type": "enumeration",
      "enum": [
        "grey",
        "Old Pink",
        "Davy Black",
        "Periwinkle Grey",
        "Greenish Yellow",
        "Brown",
        "Pink Flare",
        "Dark Grey"
      ],
      "required": true
    },
    "qty": {
      "type": "integer"
    },
    "size": {
      "type": "float"
    },
    "status": {
      "type": "boolean"
    }
  }
}
